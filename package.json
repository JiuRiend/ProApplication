{
  "name": "react-application",
  "version": "0.1.0",
  "description": "使用 TypeScript，包含大量 UI 区块，比如图表、表单等。",
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "prepare": "husky install",
    "preinstall": "node ./scripts/preinstall.js",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache",
    "start": "cross-env UMI_ENV=app umi dev",
    "build": "cross-env UMI_ENV=app umi build",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "version": "node ./scripts/gen_version.js",
    "bootstrap": "node ./scripts/bootstrap",
    "dev-lib": "node ./scripts/checkEnv && node ./scripts/devPackage.js",
    "build-lib": "node ./scripts/checkEnv && node ./scripts/checkDeps && node ./scripts/buildAdapt",
    "build-selectPkg": "node ./scripts/checkEnv && node ./scripts/buildPackage",
    "build-dist": "node ./scripts/checkEnv && node ./scripts/checkDeps && webpack  && node ./scripts/gen_less_entry.js",
    "docs": "cross-env  APP_ROOT=.dumi  dumi dev",
    "build-doc": "node ./scripts/checkEnv && cross-env SITE_DEPLOY='TRUE'  APP_ROOT=.dumi dumi build && mv -f build-docs build",
    "dash": "node ./scripts/checkEnv && cd packages/dashboard && npm run start",
    "build-onlyDoc": "node ./scripts/checkEnv && cross-env SITE_DEPLOY='TRUE' dumi build ",
    "release": "pnpm build-lib && pnpm build-dist &&  npm test &&  pnpm changeset && pnpm changeset version && node ./scripts/createRelease.js && npm run release:only && build-doc",
    "release:only": "node ./scripts/checkDeps && node ./scripts/preDeploy && pnpm changeset publish --registry=http://10.28.184.132:4837/",
    "change-log": "node ./scripts/createRelease.js ",
    "commit": "node ./scripts/checkDeps && pnpm changeset",
    "test": "node ./scripts/checkDeps && jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@ant-design/plots": "^1.0.9",
    "@ant-design/pro-components": "^1.1.9",
    "@monaco-editor/react": "^4.4.5",
    "@vis/charts": "workspace:^*",
    "@vis/common": "workspace:^*",
    "@vis/components": "workspace:^*",
    "@vis/test": "workspace:^*",
    "@vis/utils": "workspace:^*",
    "ahooks": "^3.3.10",
    "antd": "^4.20.6",
    "copy-text-to-clipboard": "^3.0.1",
    "crypto-js": "^4.1.1",
    "darkreader": "^4.9.57",
    "html2canvas": "^1.4.1",
    "jsencrypt": "^3.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "prop-types": "^15.5.8",
    "pubsub-js": "^1.9.4",
    "react": "^17.0.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-intl": "^2.8.0",
    "react-rnd": "^10.3.7",
    "react-sortable-hoc": "^2.0.0",
    "scheduler": "^0.23.0",
    "styled-components": "^5.3.5",
    "use-context-selector": "^1.4.1"
  },
  "resolutions": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "@types/react": ">=16.9.0",
    "@types/react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.12.2",
    "build-plugin-dev-inspector": "0.1.2",
    "build-plugin-fusion": "^0.1.22",
    "build-plugin-icestark": "^2.0.0",
    "build-plugin-keep-alive": "^1.8.1",
    "build-plugin-moment-locales": "^0.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "dumi": "1.1.43",
    "eslint": "^7.30.0",
    "father-build": "^1.22.3",
    "husky": "^8.0.1",
    "jest": "^27.3.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-helmet-async": "^1.0.9",
    "style-loader": "^2.0.0",
    "stylelint": "^13.2.0",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0"
  },
  "repository": {
    "type": "git",
    "url": "http://10.28.184.132/ssa-vis/vis-react"
  },
  "private": true,
  "originTemplate": "@alifd/fusion-design-pro",
  "packageManager": "pnpm@7.3.0",
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "babel": {
    "env": {
      "development": {
        "retainLines": true
      }
    }
  }
}
